{
	"name": "forrest79/phpgsql-nette",
	"description": "PhPgSql implementation into Nette Framework.",
	"keywords": ["nette", "phpgsql", "database", "postgresql"],
	"type": "library",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jakub Trmota",
			"email": "jakub@trmota.cz"
		}
	],
	"require": {
		"php":  ">=7.1",
		"forrest79/phpgsql": "~0.6",
		"nette/di":  "^2.4",
		"tracy/tracy":  "^2.4"
	},
	"require-dev": {
		"nette/tester": "~2",
		"phpstan/phpstan": "^0.11",
		"phpstan/phpstan-strict-rules": "^0.11"
	},
	"autoload": {
		"psr-4": {
			"Forrest79\\PhPgSql\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Forrest79\\PhPgSql\\Tests\\": "tests/"
		}
	}
}
